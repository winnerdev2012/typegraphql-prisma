  },
  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "apollo-server": "^2.8.2",
    "graphql": "^14.4.2",
    "prettier": "^1.18.2",
    "prisma2": "^2.0.0-preview-7",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.3.0",
    "type-graphql": "^0.17.5",
    "typescript": "^3.5.3"
  },
  "license": "MIT",
