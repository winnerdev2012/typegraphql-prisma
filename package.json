{
  "name": "typegraphql-prisma",
  "version": "0.1.2",
  "scripts": {
    "build": "tsc",
    "check:type": "tsc --noEmit",
    "check:format": "prettier --check ./**/*.{js,json,ts,tsx}",
    "test": "ts-node ./tests/helpers/ensure-engine.ts && jest --watch --verbose",
    "test:integration": "ts-node ./tests/helpers/ensure-engine.ts && jest --watch --verbose --config ./jest.config.integration.js",
    "test:ci": "ts-node ./tests/helpers/ensure-engine.ts && jest --coverage --verbose --runInBand && jest --coverage --verbose --runInBand --config ./jest.config.integration.js",
    "format": "prettier --write ./**/*.{js,json,ts,tsx}",
    "docker": "docker-compose run linux-node",
    "basic-example:docker": "docker-compose up -d --force-recreate basic-example && docker-compose logs -f basic-example"
  },
  "peerDependencies": {
    "@types/node": "*",
    "dataloader": "^2.0.0",
    "prisma2": "2.0.0-preview019",
    "type-graphql": "0.18.0-beta.8"
  },
  "dependencies": {
    "@prisma/generator-helper": "^0.0.29",
    "@prisma/photon": "2.0.0-preview019",
    "@prisma/sdk": "^0.0.105",
    "ts-morph": "^6.0.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "dataloader": "^2.0.0",
    "directory-tree": "^2.2.4",
    "graphql": "^14.5.8",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "type-graphql": "0.18.0-beta.8",
    "typescript": "^3.7.4"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.12"
  },
  "author": {
    "name": "Micha≈Ç Lytek",
    "url": "https://github.com/MichalLytek"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichalLytek/type-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/MichalLytek/type-graphql/issues"
  },
  "keywords": [
    "type-graphql",
    "prisma",
    "typegraphql",
    "prisma2",
    "prisma-framework",
    "graphql",
    "generator"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
