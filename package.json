{
  "name": "typegraphql-prisma",
  "version": "0.7.0",
  "scripts": {
    "build": "tsc",
    "check:type": "tsc --noEmit",
    "check:experiments": "cd ./experiments && tsc --noEmit",
    "check:format": "prettier --check ./**/*.{js,json,ts,tsx}",
    "test": "ts-node ./tests/helpers/setup-tests.ts && jest --watch --verbose",
    "test:integration": "ts-node ./tests/helpers/setup-tests.ts && env-cmd jest --watch --verbose --config ./jest.config.integration.js",
    "test:ci": "ts-node ./tests/helpers/setup-tests.ts && jest --coverage --verbose --runInBand && jest --coverage --verbose --runInBand --config ./jest.config.integration.js",
    "format": "prettier --write ./**/*.{js,json,ts,tsx}"
  },
  "bin": "./generator.js",
  "peerDependencies": {
    "@prisma/cli": "2.9.0-dev.36",
    "@prisma/client": "2.9.0-dev.36",
    "@types/graphql-fields": "^1.3.3",
    "@types/node": "*",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "type-graphql": "^1.0.0"
  },
  "dependencies": {
    "@prisma/generator-helper": "2.9.0-dev.36",
    "@prisma/sdk": "2.9.0-dev.36",
    "pluralize": "^8.0.0",
    "ts-morph": "^8.1.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.9.0-dev.36",
    "@prisma/client": "2.9.0-dev.36",
    "@types/graphql-fields": "^1.3.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/pg": "^7.14.5",
    "@types/pluralize": "0.0.29",
    "directory-tree": "^2.2.4",
    "env-cmd": "^10.1.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "pg": "^8.3.3",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "~26.4.1",
    "ts-node": "^9.0.0",
    "type-graphql": "^1.0.0",
    "typescript": "~4.0.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.4"
  },
  "author": {
    "name": "Micha≈Ç Lytek",
    "url": "https://github.com/MichalLytek"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichalLytek/typegraphql-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/MichalLytek/typegraphql-prisma/issues"
  },
  "keywords": [
    "type-graphql",
    "typegraphql",
    "prisma",
    "prisma2",
    "prisma-framework",
    "graphql",
    "generator"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{js,json}": [
      "prettier --write"
    ]
  },
  "private": true
}
