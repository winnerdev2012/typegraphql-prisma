{
  "name": "typegraphql-prisma",
  "version": "0.2.0",
  "scripts": {
    "build": "tsc",
    "check:type": "tsc --noEmit",
    "check:format": "prettier --check ./**/*.{js,json,ts,tsx}",
    "test": "ts-node ./tests/helpers/setup-tests.ts && jest --watch --verbose",
    "test:integration": "ts-node ./tests/helpers/setup-tests.ts && jest --watch --verbose --config ./jest.config.integration.js",
    "test:ci": "ts-node ./tests/helpers/setup-tests.ts && jest --coverage --verbose --runInBand && jest --coverage --verbose --runInBand --config ./jest.config.integration.js",
    "format": "prettier --write ./**/*.{js,json,ts,tsx}",
    "docker": "docker-compose run linux-node",
    "basic-example:docker": "docker-compose up -d --force-recreate basic-example && docker-compose logs -f basic-example"
  },
  "peerDependencies": {
    "@prisma/cli": "2.0.0-beta.2",
    "@prisma/client": "2.0.0-beta.2",
    "@types/node": "*",
    "type-graphql": "0.18.0-beta.16"
  },
  "dependencies": {
    "@prisma/generator-helper": "2.0.0-beta.2",
    "@prisma/sdk": "2.0.0-beta.2",
    "ts-morph": "^7.0.2",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.2",
    "@prisma/client": "2.0.0-beta.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "directory-tree": "^2.2.4",
    "graphql": "^14.6.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "type-graphql": "0.18.0-beta.16",
    "typescript": "^3.8.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.12"
  },
  "author": {
    "name": "Micha≈Ç Lytek",
    "url": "https://github.com/MichalLytek"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichalLytek/type-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/MichalLytek/type-graphql/issues"
  },
  "keywords": [
    "type-graphql",
    "typegraphql",
    "prisma",
    "prisma2",
    "prisma-framework",
    "graphql",
    "generator"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{js,json}": [
      "prettier --write"
    ]
  }
}
